{
  "kind": "collectionType",
  "collectionName": "vendors",
  "info": {
    "name": "Vendor",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "port_pointer": {
      "type": "integer",
      "unique": true
    },
    "Name": {
      "type": "string",
      "unique": true
    },
    "domain": {
      "type": "string",
      "regex": "^[a-zA-Z0-9][a-zA-Z0-9-]{1,61}[a-zA-Z0-9]\\.[a-zA-Z]{2,}$",
      "unique": true,
      "required": true
    },
    "registration_url": {
      "type": "string"
    },
    "plan_type": {
      "type": "enumeration",
      "enum": [
        "monthly_fee",
        "percentage"
      ],
      "default": "monthly_fee",
      "required": true
    },
    "fee_amount": {
      "type": "decimal",
      "default": 0,
      "required": true,
      "min": 0
    }
  }
}
