{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "waiting",
        "unconfirmed",
        "confirmed",
        "canceled"
      ]
    },
    "products": {
      "type": "json",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "pickup",
        "delivery"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "note": {
      "type": "string"
    }
  }
}
