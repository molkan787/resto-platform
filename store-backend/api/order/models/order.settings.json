{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "placed",
        "confirmed",
        "canceled",
        "approved",
        "declined",
        "ready",
        "shipped",
        "delivered"
      ]
    },
    "products": {
      "type": "json",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "pickup",
        "delivery"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "note": {
      "type": "string"
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
